<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="proiect.proiect_sociu_lucian.YourPolls"
      spacing="10"
      alignment="center">

    <Label fx:id="titleLabel" text="Poll-urile tale" />

    <TableView fx:id="dataTable">
        <TableColumn  text="Topic" fx:id="topic"/>
        <TableColumn text="Numar total de voturi" fx:id="nrTotalVoturi"/>
        <TableColumn text="Numar Voturi" fx:id="nrVoturi"/>
        <TableColumn text="Castigator" fx:id="winner"/>
    </TableView>

    <Button fx:id="homeButton" text="Home" />

</VBox>
